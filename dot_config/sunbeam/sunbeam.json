			"origin": "https://raw.githubusercontent.com/pomdtr/sunbeam/main/extensions/gists.ts"
		},
		"google": {
			"origin": "https://raw.githubusercontent.com/pomdtr/sunbeam/main/extensions/google.sh"
		}
