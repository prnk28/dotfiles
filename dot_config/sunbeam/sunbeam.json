{
	"oneliners": [
		{
			"title": "Github: Actions",
			"command": "uwsm app -- ~/.local/share/applications/github-actions.desktop",
			"exit": true
		},
		{
			"title": "Github: Issues",
			"command": "uwsm app -- ~/.local/share/applications/github-issues.desktop",
			"exit": true
		},
		{
			"title": "Github: Pull Requests",
			"command": "uwsm app -- ~/.local/share/applications/github-prs.desktop",
			"exit": true
